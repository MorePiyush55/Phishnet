{
  "version": 2,
  "name": "phishnet-frontend", 
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "env": {
    "VITE_BACKEND_URL": "https://phishnet-backend.onrender.com",
    "VITE_API_BASE_URL": "https://phishnet-backend.onrender.com/api"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}