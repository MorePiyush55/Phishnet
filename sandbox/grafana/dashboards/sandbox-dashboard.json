{
  "dashboard": {
    "id": null,
    "title": "PhishNet Sandbox Dashboard",
    "tags": ["sandbox", "security", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Job Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sandbox_jobs_total[5m])",
            "legendFormat": "Jobs/sec - {{status}}"
          }
        ],
        "xAxis": {"show": true},
        "yAxes": [
          {
            "label": "Jobs per second",
            "show": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Queue Size",
        "type": "stat",
        "targets": [
          {
            "expr": "sandbox_queue_size",
            "legendFormat": "Pending Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "sandbox_active_workers",
            "legendFormat": "Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Job Duration",
        "type": "histogram",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(sandbox_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(sandbox_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sandbox_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory MB - {{instance}}"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "show": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sandbox_cpu_usage_percent",
            "legendFormat": "CPU % - {{instance}}"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "max": 100,
            "min": 0,
            "show": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sandbox_security_events_total[5m])",
            "legendFormat": "Security Events/sec - {{event_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Events per second",
            "show": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Blocked Requests",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sandbox_blocked_requests_total)",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Artifacts Stored",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sandbox_artifacts_stored_total[5m])",
            "legendFormat": "Artifacts/sec - {{artifact_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
}
